<h2>Here are all the clients for this stylist:</h2>

@if (@Model["client"].Count == 0)
{
  <p>No clients have been added yet!</p>
}
else
{
  <ol>
    @foreach (var client in @Model["client"])
    {
      <li>@client.GetClientName()</li>
    }
  </ol>
}
<p><a href="/clients/new">Add a new client</a></p>
<p><a href="/">Return to Main Page</a></p>
